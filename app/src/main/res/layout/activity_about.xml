                    android:id="@+id/content_tv"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:autoSizeMaxTextSize="18dp"
                    android:autoSizeMinTextSize="1dp"
                    android:autoSizeTextType="uniform"
